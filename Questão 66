num = int(input('Insira um número: '))
q = 0
d = [0]
for x in range (1, num+1):
    if num%x==0:
        q += 1
        d.append(x)
    lista = (str(d)[1:-1])
if q == 2:
    print ('O número inserido é um número primo.')
else:
    print ('O número inserido não é um número primo, e é divisível pelos números ' + lista)
